{
    "name": "jami-scrapi",
    "description": "Scraping CLI",
    "version": "2.1.0",
    "author": "Ragaeeb Haq",
    "license": "MIT",
    "private": false,
    "source": "src/index.ts",
    "main": "dist/main.js",
    "type": "module",
    "bin": {
        "jami-scrapi": "dist/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ragaeeb/jami-scrapi.git"
    },
    "engines": {
        "node": ">=23.0.0"
    },
    "files": [
        "dist/index.js"
    ],
    "scripts": {
        "start": "bun run src/index.ts",
        "build": "NODE_ENV=production bun build src/index.ts --outdir dist --target node --packages=external",
        "compile": "NODE_ENV=production bun build src/index.ts --target node --compile --outfile dist/jami-scrapi",
        "test": "vitest run"
    },
    "keywords": [
        "scraping"
    ],
    "devDependencies": {
        "@eslint/js": "^9.21.0",
        "@semantic-release/changelog": "^6.0.3",
        "@types/cli-progress": "^3.11.6",
        "@types/node": "^22.13.5",
        "@vitest/coverage-v8": "^3.0.7",
        "eslint": "^9.21.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-perfectionist": "^4.9.0",
        "eslint-plugin-prettier": "^5.2.3",
        "prettier": "^3.5.2",
        "semantic-release": "^24.2.3",
        "typescript-eslint": "^8.25.0",
        "vitest": "^3.0.7"
    },
    "dependencies": {
        "@inquirer/prompts": "^7.3.2",
        "bimbimba": "latest",
        "cli-welcome": "^3.0.1",
        "pino": "^9.6.0",
        "pino-pretty": "^13.0.0"
    }
}
