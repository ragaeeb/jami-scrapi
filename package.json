{
    "name": "jami-scrapi",
    "description": "Scraping CLI",
    "version": "2.5.0",
    "author": "Ragaeeb Haq",
    "license": "MIT",
    "private": false,
    "source": "src/index.ts",
    "main": "dist/main.js",
    "type": "module",
    "bin": {
        "jami-scrapi": "dist/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ragaeeb/jami-scrapi.git"
    },
    "engines": {
        "bun": ">=1.2.20",
        "node": ">=23.0.0"
    },
    "files": [
        "dist/index.js"
    ],
    "scripts": {
        "start": "bun run src/index.ts",
        "build": "NODE_ENV=production bun build src/index.ts --outdir dist --target node --packages=external",
        "compile": "NODE_ENV=production bun build src/index.ts --target node --compile --outfile dist/jami-scrapi",
        "test": "bun test"
    },
    "keywords": [
        "scraping"
    ],
    "devDependencies": {
        "@eslint/js": "^9.33.0",
        "@types/bun": "^1.2.20",
        "@types/node": "^24.2.1",
        "eslint": "^9.33.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-perfectionist": "^4.15.0",
        "eslint-plugin-prettier": "^5.5.4",
        "prettier": "^3.6.2",
        "semantic-release": "^24.2.7",
        "typescript-eslint": "^8.39.1"
    },
    "dependencies": {
        "@inquirer/prompts": "^7.8.2",
        "bimbimba": "^0.11.0",
        "bitaboom": "^1.4.0",
        "catsa-janga": "^1.0.1",
        "cli-welcome": "^3.0.1",
        "pino": "^9.9.0",
        "pino-pretty": "^13.1.1"
    }
}
